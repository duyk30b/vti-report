{
  "name": "report-service",
  "version": "0.0.1",
  "description": "",
  "author": "VTI.SnP",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "postinstall": "husky install",
    "build": "nest build  &&  npm run copy-assets",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/config/database.config.ts",
    "migrate:generate": "npm run typeorm migration:generate -- -n",
    "migrate:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:create ",
    "migrate:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/config/database.config.ts",
    "migrate:revert": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d src/config/database.config.ts",
    "copy-assets": "cp -rv src/i18n dist"
  },
  "dependencies": {
    "@fastify/cors": "7.0.0",
    "@nestcloud/boot": "0.7.17",
    "@nestcloud/common": "0.7.17",
    "@nestcloud/consul": "0.7.17",
    "@nestcloud/etcd": "0.7.17",
    "@nestcloud/service": "0.7.17",
    "@nestjs/axios": "0.0.8",
    "@nestjs/common": "8.4.7",
    "@nestjs/config": "2.1.0",
    "@nestjs/core": "8.4.7",
    "@nestjs/event-emitter": "1.1.1",
    "@nestjs/microservices": "8.4.7",
    "@nestjs/platform-fastify": "^8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "8.1.4",
    "big.js": "6.2.0",
    "class-transformer": "^0.5.1",
    "cache-manager": "4.0.1",
    "cache-manager-redis-store": "2.0.0",
    "redis": "3.1.2",
    "class-validator": "0.13.2",
    "exceljs": "4.3.0",
    "fastify-multipart": "^5.4.0",
    "lodash": "4.17.21",
    "moment": "2.29.3",
    "moment-range": "4.0.2",
    "nestjs-i18n": "9.1.2",
    "papaparse": "5.3.2",
    "rxjs": "^7.5.5",
    "@nestjs/mongoose": "9.1.1",
    "mongoose": "6.0.13",
    "husky": "^8.0.1",
    "fastify-swagger": "4.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "type": "module",
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
